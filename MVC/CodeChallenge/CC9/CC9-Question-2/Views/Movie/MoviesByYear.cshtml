@model IEnumerable<CC9_Question_2.Models.Movie>
@{
    ViewBag.Title = "MoviesByYear";
}

<h2>Movies Released in @ViewBag.Year</h2>

<table class="table">
    <tr>
        <th>Movie Name</th>
        <th>Director</th>
        <th>Date of Release</th>
    </tr>

    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.MovieName</td>
            <td>@movie.DirectorName</td>
            <td>@movie.DateOfRelease.ToShortDateString()</td>
        </tr>
    }
</table>


